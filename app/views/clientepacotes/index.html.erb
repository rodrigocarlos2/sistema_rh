<p id="notice"><%= notice %></p>

<h1>Clientepacotes</h1>

<table>
  <thead>
    <tr>
      <th>Client</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientepacotes.each do |clientepacote| %>
      <tr>
        <td><%= clientepacote.client %></td>
        <td><%= link_to 'Show', clientepacote %></td>
        <td><%= link_to 'Edit', edit_clientepacote_path(clientepacote) %></td>
        <td><%= link_to 'Destroy', clientepacote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clientepacote', new_clientepacote_path %>
